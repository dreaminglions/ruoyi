<mapper namespace="com.ruoyi.system.mapper.BizAssayCurveMapper">
    
    <resultMap type="BizAssayCurve" id="BizAssayCurveResult">
        <result property="curveId"    column="curve_id"    />
        <result property="reportId"    column="report_id"    />
        <result property="curveTime"    column="curve_time"    />
        <result property="tnWave1"    column="tn_wave1"    />
        <result property="tnWave2"    column="tn_wave2"    />
        <result property="tnCurveK"    column="tn_curve_k"    />
        <result property="tnCurveB"    column="tn_curve_b"    />
        <result property="tpWave1"    column="tp_wave1"    />
        <result property="tpWave2"    column="tp_wave2"    />
        <result property="tpCurveK"    column="tp_curve_k"    />
        <result property="tpCurveB"    column="tp_curve_b"    />
        <result property="nhWave1"    column="nh_wave1"    />
        <result property="nhWave2"    column="nh_wave2"    />
        <result property="nhCurveK"    column="nh_curve_k"    />
        <result property="nhCurveB"    column="nh_curve_b"    />
        <result property="incodWave1"    column="incod_wave1"    />
